<?xml version="1.0" encoding="ISO-8859-1"?>
<web-app>

    <servlet>
        <servlet-name>login</servlet-name>
        <servlet-class>login</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>logout</servlet-name>
        <servlet-class>logout</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>changePassword</servlet-name>
        <servlet-class>changePassword</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>PatientsList</servlet-name>
        <servlet-class>PatientsList</servlet-class>
    </servlet>
	
	<servlet>
        <servlet-name>PatientsUpdate</servlet-name>
        <servlet-class>PatientsUpdate</servlet-class>
    </servlet>
	
	<servlet>
        <servlet-name>PatientsEdit</servlet-name>
        <servlet-class>PatientsEdit</servlet-class>
    </servlet>
	
    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>logout</servlet-name>
        <url-pattern>/logout</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>changePassword</servlet-name>
        <url-pattern>/changePassword</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>PatientsList</servlet-name>
        <url-pattern>/PatientsList</url-pattern>
    </servlet-mapping>
	
	<servlet-mapping>
        <servlet-name>PatientsUpdate</servlet-name>
        <url-pattern>/PatientsUpdate</url-pattern>
    </servlet-mapping>
	
	<servlet-mapping>
        <servlet-name>PatientsEdit</servlet-name>
        <url-pattern>/PatientsEdit</url-pattern>
    </servlet-mapping>

</web-app>
